<?xml version="1.0" encoding="UTF-8"?>
<concrete5-cif version="1.0">
    <themes>
        <theme handle="concrete_cms" package="concrete_cms_theme" activated="1"/>
    </themes>
    <stacks>
        <stack name="Register Complete" path="/register-complete" />
    </stacks>
    <blocktypes>
        <blocktype handle="feature_item" package="concrete_cms_theme"/>
        <blocktype handle="image_with_quote" package="concrete_cms_theme"/>
        <blocktype handle="statistics" package="concrete_cms_theme"/>
    </blocktypes>
    <pagetemplates>
        <pagetemplate icon="full.png" name="Case Study" handle="case_study" package="" internal=""/>
        <pagetemplate icon="full.png" name="Blog Entry (Wide)" handle="blog_entry_wide" package="" internal=""/>
        <pagetemplate icon="full.png" name="Full Text" handle="full_text" package="" internal=""/>
    </pagetemplates>
    <attributekeys>
        <attributekey handle="developer_name" name="Developer Name" package="concrete_cms_marketing" searchable="1"
                      indexed="1" type="text"
                      category="collection"/>
        <attributekey handle="developer_url" name="Developer URL" package="concrete_cms_marketing" searchable="1"
                      indexed="1" type="text"
                      category="collection"/>
        <attributekey handle="site_name" name="Site Name" package="concrete_cms_marketing" searchable="1" indexed="1"
                      type="text"
                      category="collection"/>
        <attributekey handle="site_url" name="Site URL" package="concrete_cms_marketing" searchable="1" indexed="1"
                      type="text"
                      category="collection"/>
        <attributekey handle="blog_entry_call_to_action_text" name="Blog Entry CTA Text" package="concrete_cms_marketing" searchable="1"
                      indexed="1" type="text"
                      category="collection"/>
        <attributekey handle="blog_entry_call_to_action_url" name="Blog Entry CTA URL" package="concrete_cms_marketing" searchable="1"
                      indexed="1" type="text"
                      category="collection"/>
    </attributekeys>
    <thumbnailtypes>
        <thumbnailtype name="Case Study Thumbnail" handle="case_study_thumbnail" width="672" height="464"
                       sizingMode="exact"/>
    </thumbnailtypes>
    <summarytemplates>
        <template handle="case_study_item" name="Case Study Item"
                  icon="blank.png" package="concrete_cms_theme">
            <categories>
                <category handle="page"/>
            </categories>
            <fields>
                <field required="1">title</field>
                <field required="1">link</field>
                <field required="0">thumbnail</field>
                <field>description</field>
            </fields>
        </template>
    </summarytemplates>
    <attributekeys>
        <attributekey handle="enable_dark_mode" name="Enable Dark Mode" package="concrete_cms_theme"
                      searchable="1"
                      indexed="0" type="boolean"
                      category="collection"/>
        <attributekey handle="header_image" name="Header Image" package="concrete_cms_theme" searchable="1" indexed="1"
                      type="image_file"
                      category="user">
            <type placeholder=""/>
        </attributekey>
    </attributekeys>
    <singlepages>
        <page name="ConcreteCMS Theme" path="/dashboard/concrete_cms_theme" filename="/dashboard/concrete_cms_theme.php"
              pagetype="" description=""
              package="concrete_cms_theme">
        </page>
        <page name="Settings" path="/dashboard/concrete_cms_theme/settings"
              filename="/dashboard/concrete_cms_theme/settings.php" pagetype="" description=""
              package="concrete_cms_theme">
        </page>
    </singlepages>
    <containers>
        <container handle="masthead_larger_right_image_text" name="Masthead Right Image (Large) with Text" icon="feature.png" package=""/>
        <container handle="masthead_larger_left_image_text" name="Masthead Left Image (Large) with Text" icon="feature.png" package=""/>
        <container handle="masthead_left_image_text" name="Masthead Left Image with Text" icon="feature.png" package=""/>
        <container handle="masthead_right_image_text" name="Masthead Right Image with Text" icon="feature.png" package=""/>
        <container handle="feature_list_section" name="Feature List Section" icon="left_list.png" package=""/>
        <container handle="two_column_white" name="Two Column White" icon="two_column.png" package=""/>
        <container handle="two_column_light_gray" name="Two Column Light Gray" icon="two_column.png" package=""/>
        <container handle="two_column_dark_gray" name="Two Column Dark Gray" icon="two_column.png" package=""/>
        <container handle="two_column_blue" name="Two Column Blue" icon="two_column.png" package=""/>

        <container handle="two_column_48_white" name="Two Column (1/3 &amp; 2/3) White" icon="two_column.png" package=""/>
        <container handle="two_column_48_light_gray" name="Two Column (1/3 &amp; 2/3) Light Gray" icon="two_column.png" package=""/>
        <container handle="two_column_48_dark_gray" name="Two Column (1/3 &amp; 2/3) Dark Gray" icon="two_column.png" package=""/>
        <container handle="two_column_48_blue" name="Two Column (1/3 &amp; 2/3) Blue" icon="two_column.png" package=""/>

        <container handle="two_column_top_align_white" name="Two Column (Top Align) White" icon="two_column.png" package=""/>
        <container handle="two_column_top_align_light_gray" name="Two Column (Top Align) Light Gray" icon="two_column.png" package=""/>
        <container handle="two_column_top_align_dark_gray" name="Two Column (Top Align) Dark Gray" icon="two_column.png" package=""/>
        <container handle="two_column_top_align_blue" name="Two Column (Top Align) Blue" icon="two_column.png" package=""/>

        <container handle="three_column_white" name="Three Column White" icon="three_column.png" package=""/>
        <container handle="three_column_light_gray" name="Three Column Light Gray" icon="three_column.png" package=""/>
        <container handle="three_column_dark_gray" name="Three Column Dark Gray" icon="three_column.png" package=""/>
        <container handle="three_column_blue" name="Three Column Blue" icon="three_column.png" package=""/>
        <container handle="case_study_landing_header" name="Case Study Landing Header" icon="two_column.png" package=""/>
        <container handle="section_light_gray" name="Light Gray Single Column" icon="two_column.png" package=""/>
        <container handle="section_white" name="White Single Column" icon="two_column.png" package=""/>
        <container handle="section_blue" name="Blue Single Column" icon="two_column.png" package=""/>
        <container handle="section_dark_gray" name="Dark Gray Single Column" icon="two_column.png" package=""/>
        <container handle="section_accent" name="Accent Single Column" icon="two_column.png" package=""/>
        <container handle="solution_section_white" name="Solution Section White" icon="two_column.png" package=""/>
        <container handle="solution_section_light_gray" name="Solution Section Light Gray" icon="two_column.png" package=""/>

        <container handle="three_column_hosting_white" name="Three Column Hosting White" icon="three_column.png" package=""/>

    </containers>
</concrete5-cif>